{
    "name": "programic/laravel-tools",
    "license": "proprietary",
    "type": "package",
    "description": "Laravel extension",
    "keywords": [
        "programic",
        "laravel"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "Programic",
            "email": "development@programic.com",
            "homepage": "https://programic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6|^7.0|^8.0",
        "laravel/framework" : "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "sentry/sentry-laravel": "^2.14|^3.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.1 | ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Programic\\Tools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programic\\Tools\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Programic\\Tools\\ToolsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
