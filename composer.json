{
    "name": "programic/laravel-tools",
    "license": "proprietary",
    "type": "package",
    "description": "Laravel extension",
    "keywords": [
        "programic",
        "laravel"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "Programic",
            "email": "development@programic.com",
            "homepage": "https://programic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "sentry/sentry-laravel": "^2.14|^3.2",
        "spatie/laravel-health": "^1.22",
        "barryvdh/laravel-debugbar": "^3.9"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.1|^8.0",
        "phpunit/phpunit": "^8.5.23 || ^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Programic\\Tools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programic\\Tools\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Programic\\Tools\\ToolsServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
